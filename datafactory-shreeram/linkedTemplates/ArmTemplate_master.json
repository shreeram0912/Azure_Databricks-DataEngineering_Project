{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-shreeram"
		},
		"LinkedService_AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LinkedService_AzureDatabricks'"
		},
		"linkedservice_azuresqldatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedservice_azuresqldatabase'"
		},
		"linkedservice_datalake_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedservice_datalake_gen2'"
		},
		"LinkedService_AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0222-191616-4vfxshi3"
		},
		"linkedservice_azuresqldatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "server-azuresqldatabase.database.windows.net"
		},
		"linkedservice_azuresqldatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "database-azure-sql"
		},
		"linkedservice_azuresqldatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ram"
		},
		"linkedservice_datalake_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeazuredatabricks.dfs.core.windows.net/"
		},
		"linkedservice_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-shreeram_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedService_AzureDatabricks_accessToken": {
						"value": "[parameters('LinkedService_AzureDatabricks_accessToken')]"
					},
					"linkedservice_azuresqldatabase_password": {
						"value": "[parameters('linkedservice_azuresqldatabase_password')]"
					},
					"linkedservice_datalake_gen2_accountKey": {
						"value": "[parameters('linkedservice_datalake_gen2_accountKey')]"
					},
					"LinkedService_AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LinkedService_AzureDatabricks_properties_typeProperties_existingClusterId')]"
					},
					"linkedservice_azuresqldatabase_properties_typeProperties_server": {
						"value": "[parameters('linkedservice_azuresqldatabase_properties_typeProperties_server')]"
					},
					"linkedservice_azuresqldatabase_properties_typeProperties_database": {
						"value": "[parameters('linkedservice_azuresqldatabase_properties_typeProperties_database')]"
					},
					"linkedservice_azuresqldatabase_properties_typeProperties_userName": {
						"value": "[parameters('linkedservice_azuresqldatabase_properties_typeProperties_userName')]"
					},
					"linkedservice_datalake_gen2_properties_typeProperties_url": {
						"value": "[parameters('linkedservice_datalake_gen2_properties_typeProperties_url')]"
					},
					"linkedservice_github_properties_typeProperties_url": {
						"value": "[parameters('linkedservice_github_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}